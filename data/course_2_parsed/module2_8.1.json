  "suffixes": [
    {
      "form": "-ma",
      "meaning": "negation of adjectives or nouns; for verbs: to make an action happen, causative",
      "usage": "attached to adjectives, nouns, and verbs; does not indicate tense and requires accompanying tense marker",
      "examples": [
        "jakonma (not good)",
        "oinmai (make see, with -ai present tense)",
        "nashimai (make bathe, with -ai present tense)",
        "oinmabanon (will make see, with -banon announcement tense)",
        "nashimabanon (will make bathe, with -banon announcement tense)"
      ]
    },
    {
      "form": "-we",
      "meaning": "singular imperative, singular command",
      "usage": "command directed to a single person; widely used in icaros and day-to-day language; can be combined with -ma",
      "examples": [
        "oinwe (look!)",
        "nashiwe (bathe!)",
        "oinmawe (make me look, see!)",
        "nashimawe (make me bathe, bathe me!)",
        "ninkamawe (make me hear!)",
        "yoiwe (speak!)",
        "tanamawe (make me try!)",
        "amawe (make me offer!)",
        "oinmawe (make me see!)",
        "nashimawe (make me bathe!)"
      ]
    },
    {
      "form": "-ai",
      "meaning": "present tense marker",
      "usage": "indicates present tense; used with verb stems that have taken -ma suffix",
      "examples": [
        "oinmai (make see, present)",
        "nashimai (make bathe, present)"
      ]
    },
    {
      "form": "-banon",
      "meaning": "announcement of an action that will happen immediately after",
      "usage": "tense marker; indicates immediate future; does not require -ra with first subject pronoun",
      "examples": [
        "oinmabanon (will make see, will show)",
        "nashimabanon (will make bathe)",
        "ninkamabanon (will make hear)",
        "xeamabanon (will make heal/bathe, implied)",
        "kinanmabanon (will make do, implied)",
        "oinmabanon (will make see)",
        "nashimabanon (will make bathe)"
      ]
    }
